using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using System;

public class Sombrero : MonoBehaviour
{
    // Start is called before the first frame update
    
    void Start()
    {
static void Mano(string[] args)
        {
            Debug.Log("combinedText");
        
        }
        string[] myArgs = new string[] { "arg1", "arg2", "arg3" }; // Create an example array of strings
        Mano(myArgs); // Call the Mano method with the array of strings as argument
    }

    // Custom method with string array parameter
    static void Mano(string[] args)
    {
        // Access and print the elements of the string array
        for (int i = 0; i < args.Length; i++)
        {
            Debug.Log("Arg " + i + ": " + args[i]);
        }
    }

    // Update is called once per frame
    void Update()
    {
          
    }
}
